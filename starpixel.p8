pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
--initializing global variables

ship = {}
enemies = {}
bullets = {}
enemy_interval = 15

function _init()

	--initialize ship
	ship.x = 64
	ship.y = 118
	ship.sprite = 1
	ship.hspd = 2
	ship.shoot = 0 --interval between shoot
	
	music(0)
	
end

function _update()
	--controls
	if (btn(0))	ship.x = ship.x-ship.hspd
	if (btn(1)) ship.x = ship.x+ship.hspd
	if (btnp(5) and ship.shoot == 0) shoot()
	if (ship.shoot > 0) shoot_counter()
	--boundaries	
	if (ship.x > 120) ship.x = 120
	if (ship.x < 8) ship.x = 8
	
	--bullet control
	if (#bullets > 0) update_bullets()
	
	--enemy control
	if (#enemies > 0) enemy_update()
	enemy_timer()
end

function _draw()
	cls()
	spr(1,ship.x,ship.y)
	if #bullets > 0 then
		for bullet in all(bullets) do
			line(bullet.x, bullet.y1, bullet.x, bullet.y2,11)
		end
	end
	if #enemies > 0 then
		for enemy in all(enemies) do
			spr(2,enemy.x, enemy.y)
		end
	end
end

function create_bullet()
	add(bullets,{})
	
	bullets[#bullets].x = ship.x+3
	bullets[#bullets].speed = 1
	bullets[#bullets].length = 4
	bullets[#bullets].y1 = ship.y + 2
	bullets[#bullets].y2 = bullets[#bullets].y1 - bullets[#bullets].length
end

function update_bullets()
	for bullet in all(bullets) do
			bullet.y1 = bullet.y1 - bullet.speed
			bullet.y2 = bullet.y1 - bullet.length
			if (bullet.y1 < 0) del(bullets, bullet)
	end
end

function shoot()
	if ship.shoot == 0 then
		create_bullet()
		ship.shoot = 10
		sfx(0,0)
	end
end

function shoot_counter()
		ship.shoot = ship.shoot - 1
end

function enemy_spawn()
	add(enemies,{})
	enemies[#enemies].x = flr(rnd(100))+8
	enemies[#enemies].y = -6
end

function enemy_update()
	for enemy in all(enemies) do
		enemy.y = enemy.y + 1
		if (enemy.y > 132) del(enemies, enemy)
	end
end

function enemy_timer()
	if enemy_interval == 0 then
		enemy_spawn()
		enemy_interval = 30
	else
		enemy_interval = enemy_interval - 1
	end
end
