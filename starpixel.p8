pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
--initializing global variables

ship = {}
enemies = {}
bullets = {}
explosions = {}
enemy_interval = 15

function _init()

	--initialize ship
	ship.x = 64
	ship.y = 118
	ship.sprite = 1
	ship.hspd = 2
	ship.shoot = 0 --interval between shoot
	
	music(0)
	
end

function _update()
	--controls
	if (btn(0))	ship.x = ship.x-ship.hspd
	if (btn(1)) ship.x = ship.x+ship.hspd
	if (btnp(5) and ship.shoot == 0) shoot()
	if (ship.shoot > 0) shoot_counter()
	
	--boundaries	
	if (ship.x > 120) ship.x = 120
	if (ship.x < 8) ship.x = 8
	
	--bullet control
	if (#bullets > 0) update_bullets()
	
	--enemy control
	if (#enemies > 0) enemy_update()
	enemy_timer()
	
	if (#explosions > 0) expl_update()
	
	if #bullets > 0 and #enemies > 0 then
		for b in all(bullets) do
			for e in all(enemies) do
				collision(b,e)
			end
		end
	end
	
end

function _draw()
	cls()
	--draw player
	spr(1,ship.x,ship.y)
	
	--draw bullets
	if #bullets > 0 then
		for bullet in all(bullets) do
			line(bullet.x, bullet.y, bullet.x, bullet.y-bullet.h,11)
		end
	end
	
	--draw enemies
	if #enemies > 0 then
		for enemy in all(enemies) do
			spr(enemy.sprite,enemy.x, enemy.y)
		end
	end
	
	if #explosions > 0 then
		for expl in all(explosions) do
			if (expl.r < 4) then
				circfill(expl.x, expl.y, expl.r,10)
			else
				circ(expl.x, expl.y, expl.r,9)
			end
		end
	end
end

function create_bullet()
	add(bullets,{
		x = ship.x+3,
		speed = 1,
		h = 4,
		y = ship.y
	})
end

function update_bullets()
	for bullet in all(bullets) do
			bullet.y = bullet.y - bullet.speed
			if (bullet.y < 0) del(bullets, bullet)
	end
end

function shoot()
	if ship.shoot == 0 then
		create_bullet()
		ship.shoot = 10
		sfx(0,0)
	end
end

function shoot_counter()
		ship.shoot = ship.shoot - 1
end

function enemy_spawn()
	local espr = flr(rnd(2)) + 2
	local e_width, e_height
	if (espr == 2) then
		e_width = 6
		e_height = 5
	end
	if (espr == 3) then
		e_width = 5
		e_height = 7
	end
	add(enemies,{
		x = flr(rnd(100))+8,
		y = -6,
		width = e_width,
		height = e_height,
		sprite = espr
	})

end

function enemy_update()
	for enemy in all(enemies) do
		enemy.y = enemy.y + 1
		if (enemy.y > 132) del(enemies, enemy)
	end
end

function enemy_timer()
	if enemy_interval == 0 then
		enemy_spawn()
		enemy_interval = 30
	else
		enemy_interval = enemy_interval - 1
	end
end

function collision(bullet, enemy)
	local i = 0
	if bullet.x >= enemy.x and bullet.x <= enemy.x + enemy.width then
		while (i <= bullet.h) do
			if bullet.y+i <= enemy.y+enemy.height and bullet.y+i >= enemy.y then
				i = bullet.h+1
				expl_spawn(enemy.x+flr(enemy.width/2), enemy.y + flr(enemy.height/2))
				del(bullets, bullet)
				del(enemies, enemy)
				sfx(1)
			end
			i = i+1
		end
	end
end

function expl_spawn(center_x,center_y)
	add(explosions,{
		x = center_x,
		y = center_y,
		r = 1,
		max_r = 6,
	})
end

function expl_update()
	for expl in all(explosions) do
		expl.r = expl.r+1
		if expl.r == expl.max_r then
			del(explosions, expl)
		end
	end
end
